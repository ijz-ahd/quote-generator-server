{
  "type": "postgres",
  "host": "localhost",
  "port": 5432,
  "username": "postgres",
  "password": "postgres",
  "database": "quotedb",
  "synchronize": true,
  "logging": true,
  "entities": [
    "src/entity/**/*.ts",
    "build/entity/*{.js, ..js.map}",
    "src/**/*.entity{.ts,.js}",
    "src/**/*{.entity.ts}",
    "./entity/**/*.js"
  ],
  "migrations": [
    "src/migration/**/*.ts",
    "build/migration/*{.js, .js.map}",
    "src/**/*.migration{.ts,.js}",
    "src/**/*{.migration.ts}",
    "./migration/**/*.js"
  ],
  "subscribers": ["src/subscriber/**/*.ts", "build/subscriber/*{.js, .js.map}"],
  "cli": {
    "entitiesDir": "src/entity",
    "migrationsDir": "src/migration",
    "subscribersDir": "src/subscriber"
  }
}
